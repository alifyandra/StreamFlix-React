                    onClick={() => {\n                      setCurrPage(window.location.pathname);\n                    }}\n                  >\n                    Details\n                  </Button>\n                </Link>\n              </Card.Body>\n            </Card>\n          );\n        })}\n      </CardGroup>\n    </div>\n  );\n};\n\nexport default Similar;\n","/Users/alifyandra/Documents/PersonalProjects/WebDev/react/flitts-mini-project/src/pages/Navbar.js",[],"/Users/alifyandra/Documents/PersonalProjects/WebDev/react/flitts-mini-project/src/pages/Footer.js",[],"/Users/alifyandra/Documents/PersonalProjects/WebDev/react/flitts-mini-project/src/pages/home/Home.js",["103"],{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","severity":1,"message":"109","line":19,"column":26,"nodeType":"110","messageId":"111","endLine":19,"endColumn":28},{"ruleId":"112","severity":1,"message":"113","line":27,"column":6,"nodeType":"114","endLine":27,"endColumn":23,"suggestions":"115"},{"ruleId":"112","severity":1,"message":"116","line":79,"column":6,"nodeType":"114","endLine":79,"endColumn":26,"suggestions":"117"},{"ruleId":"118","severity":1,"message":"119","line":19,"column":9,"nodeType":"120","endLine":23,"endColumn":11},{"ruleId":"108","severity":1,"message":"121","line":6,"column":15,"nodeType":"110","messageId":"111","endLine":6,"endColumn":17},{"ruleId":"108","severity":1,"message":"121","line":11,"column":20,"nodeType":"110","messageId":"111","endLine":11,"endColumn":22},{"ruleId":"108","severity":1,"message":"121","line":3,"column":19,"nodeType":"110","messageId":"111","endLine":3,"endColumn":21},{"ruleId":"108","severity":1,"message":"121","line":11,"column":23,"nodeType":"110","messageId":"111","endLine":11,"endColumn":25},{"ruleId":"112","severity":1,"message":"122","line":35,"column":6,"nodeType":"114","endLine":35,"endColumn":29,"suggestions":"123"},"no-native-reassign",["124"],"no-negated-in-lhs",["125"],"eqeqeq","Expected '